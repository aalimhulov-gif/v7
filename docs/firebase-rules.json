{
  "rules": {
    ".read": "auth != null",
    ".write": false,
    "budgets": {
      "$userId": {
        ".read": "auth != null && auth.uid == $userId",
        ".write": "auth != null && auth.uid == $userId",
        ".validate": "newData.hasChildren(['data', 'lastModified', 'version'])"
      }
    }
  }
}